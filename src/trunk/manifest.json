{
    "name": "Hover Zoom",
    "version": "1.5",
    "description": "Move the mouse cursor over a link to an image to view it in full size. Works on many sites (Facebook, Flickr, Twitter, Google, etc).",
	"icons": {
		"16": "images/icon16.png",
		"32": "images/icon32.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"background_page": "background.html",
	"options_page": "options.html",
	"page_action": {
		"default_icon": "images/icon19.png",
		"default_popup": "popup.html",
		"default_title": "Hover Zoom"
	},	
	"permissions": [
		"tabs",
		"history",
		"http://ajax.googleapis.com/",
		"http://open.api.ebay.com/"
	],
	"content_scripts": [
        {
			"matches": ["<all_urls>"],
			"css": ["hoverzoom.css"],
            "js": ["hoverzoom.js"],
			"all_frames": true
        },
        {
			"matches": ["<all_urls>"],
            "js": ["plugins/default.js"],
			"run_at": "document_start",
			"all_frames": true
        },
        {
			"matches": ["<all_urls>"],
            "js": ["plugins/imgur_a.js", 
			       "plugins/flickr_a.js",
				   "plugins/tumblr_a.js",
				   "plugins/subimg.net_a.js"],
			"run_at": "document_start",
			"all_frames": true
        },
        {
			"matches": ["*://*.facebook.com/*"],
            "js": ["plugins/facebook.js"],
			"run_at": "document_start"
        },
        {
			"matches": ["*://*.flickr.com/*"],
            "js": ["plugins/flickr.js"],
			"run_at": "document_start"
        },
        {
			"matches": ["*://*.search.yahoo.com/*",
						"*://*.search.yahoo.co.jp/*"],
